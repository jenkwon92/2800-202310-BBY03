    <!--Style -->
    <style>
        html,
        body {
            height: 100%;
            padding: 0px;
            margin: 0px;
        }

        header {
            height: 60px;
            background-color: #e9eaed;
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            padding: 0;
            margin: 0;
            z-index: 100;
        }

        #wrap {
            min-height: calc(100vh - 60px);       
            overflow-y: auto;
            -webkit-overflow-scrolling: touch;
            padding-top: 60px; /* Add top padding to account for the fixed header */
            padding-bottom: 60px; /* Add bottom padding to account for the fixed footer */
        }


        footer {
            height: 60px;
            background-color: #e9eaed;
            position: fixed;
            bottom: 0;
            left: 0;
            width: 100%;
            z-index: 100;
        }

        #plane {
            position: absolute;
            bottom: 50%;
            transform: translateX(-0%);
            left: -10%;
            width: 50px;
            height: auto;
            display: none;
        }
    </style>
</head>

<body>
    <div class="container">
        <header class="d-flex flex-wrap justify-content-center py-3 mb-4 border-bottom">
            <a href="/" class="d-flex align-items-center mb-3 mb-md-0 me-md-auto text-dark text-decoration-none">
                <img src="logo.png" alt="Logo" width="30" height="30" class="ms-2" style="margin-right: 10px;">
                CoursePilot
            </a>
        </header>
    </div>
    <div class="container" id="wrap">