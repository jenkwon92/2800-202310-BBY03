        ul.courses_tags_list li.facts-4:before {
            border-left-color: rgba(18, 163, 218, 0.13);
        }

        ul.courses_tags_list li.facts-5:before {
            border-left-color: rgba(138, 101, 251, 0.13);
        }

        ul.courses_tags_list li:after {
            background-color: #fff;
            border-radius: 50%;
            content: "";
            height: 4px;
            position: absolute;
            top: 10px;
            right: -5px;
            width: 4px;
