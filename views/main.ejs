            text-decoration: none;
            background-color: transparent;
            -webkit-text-decoration-skip: objects;
        }

        a:hover,
        a:focus {
            text-decoration: none;
        }

        a.link {
            color: #da0b4e;
        }

        a.link:hover,
        a.link:focus,
